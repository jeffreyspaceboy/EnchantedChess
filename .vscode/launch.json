{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch ChessBot",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/chess_bot.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Meson: Build",
            "miDebuggerPath": "C:/msys64/ucrt64/bin/gdb.exe",
            "logging": {
                "moduleLoad": false,
                "trace": false,
                "engineLogging": false,
                "programOutput": true,
                "exceptions": false
            },
            "targetArchitecture": "x64",
            "launchCompleteCommand": "exec-run",
            "sourceFileMap": {
                "/build/glibc-2.23": "/usr/src/glibc-2.23"
            }
        }
    ]
}
